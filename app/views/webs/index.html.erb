<h1>Listing webs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Description</th>
      <th>Private</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @webs.each do |web| %>
      <tr>
        <td><%= web.user %></td>
        <td><%= web.title %></td>
        <td><%= web.description %></td>
        <td><%= web.private %></td>
        <td><%= link_to 'Show', web %></td>
        <td><%= link_to 'Edit', edit_web_path(web) %></td>
        <td><%= link_to 'Destroy', web, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web', new_web_path %>
